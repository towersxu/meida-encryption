{
  "name": "audio-encryption",
  "version": "1.0.0",
  "description": "audio encryption and audio encryption player",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "lint": "eslint --ext .js,.vue src",
    "publish": "node ci/index.js",
    "tar": "tar cvf deploy.tar deploy.sh taox.pub && travis encrypt-file deploy.tar --com --add",
    "start": "NODE_ENV=production node .next/production-server/server.js",
    "test": "mocha -r ts-node/register"
  },
  "keywords": [
    "audio",
    "encryption"
  ],
  "author": "xutao",
  "license": "ISC",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "express": "^4.17.1",
    "formidable": "^1.2.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsmediatags": "^3.9.2",
    "next": "^9.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-markdown": "^4.2.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools": "^3.5.0",
    "signale": "^1.4.0",
    "ssh2": "^0.8.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@types/chai": "^4.2.3",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.19",
    "@types/mocha": "^5.2.7",
    "@types/next": "^8.0.6",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.9.2",
    "@types/styled-jsx": "^2.2.8",
    "@zeit/next-typescript": "^1.1.1",
    "archiver": "^3.1.1",
    "chai": "^4.2.0",
    "husky": "^3.0.9",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.3",
    "redux-devtools-extension": "^2.13.8",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": "10.x"
  }
}
